	<queryString>
		<![CDATA[SELECT 
  humane(jobworker.lastname, jobworker.firstname, jobworker.suffix, jobworker.midname) AS pangalan, 
  jobarrange.remarks
FROM 
  psnl.jobworker INNER JOIN cbo.jobarrange
  ON jobworker.uniqkey = jobarrange.empkey
WHERE
  (jobyear = DATE_PART('YEAR', NOW())) AND
  (TRIM(LOWER(jobarrange.remarks)) LIKE 'new%')
ORDER BY
  jobworker.lastname, 
  jobworker.firstname, 
  jobworker.midname;]]>
	</queryString>
	<field name="pangalan" class="java.lang.String"/>
	<field name="remarks" class="java.lang.String"/>
