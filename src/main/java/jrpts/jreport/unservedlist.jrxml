	<queryString>
		<![CDATA[SELECT
   offices.office,
   projects.projtitle,
   humane(jobworker.lastname, jobworker.firstname, jobworker.suffix, jobworker.midname),
   jobarrange.jobnum,
   jobarrange.psnl_ref,
   jobarrange.frdate AS gekan,
   jobarrange.todate AS ngadto,
   jobarrange.rate,
   jobarrange.days AS adlaw,
   jobcontrol.refno,
   jobcontrol.frdate,
   jobcontrol.todate,
   jobcontrol.days,
   jobcontrol.utime,
   (jobcontrol.rate * jobcontrol.days) AS amount
FROM 
   cbo.jobarrange LEFT JOIN cbo.jobcontrol
   ON jobarrange.jobnum = jobcontrol.jobref
   AND jobarrange.empkey = jobcontrol.empkey
   AND jobarrange.offcloc = jobcontrol.offcloc
   AND jobarrange.project = jobcontrol.project
   JOIN psnl.offices
   ON jobarrange.offcloc = offices.offcid
   JOIN psnl.jobworker
   ON jobarrange.empkey = jobworker.uniqkey
   JOIN cbo.projects
   ON jobarrange.project = projects.projid
WHERE
   (jobarrange.offcloc = $P{Opesina}) AND
   (jobarrange.todate  = $P{DatedOn})
ORDER BY
   offices.office,
   jobarrange.jobnum,
   jobworker.lastname, jobworker.firstname, jobworker.midname,
   jobcontrol.frdate,
   jobcontrol.refno]]>
	</queryString>
