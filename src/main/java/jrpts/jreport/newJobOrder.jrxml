	<queryString>
		<![CDATA[SELECT 
  jobworker.empid, 
  humane(jobworker.lastname, jobworker.firstname, jobworker.suffix, jobworker.midname) AS employees,
  jobworker.jobdesc, 
  jobworker.payrate, 
  jobarrange.frdate, 
  jobarrange.todate, 
  projects.projtitle, 
  offices.office,
  jobarrange.remarks,
  jobarrange.prep_by,
  jobarrange.prep_pos,
  jobarrange.appr_by,
  jobarrange.appr_pos,
  jobarrange.reco_by,
  jobarrange.reco_pos,
  jobarrange.cert_by,
  jobarrange.cert_pos,
  jobarrange.verify_by,
  jobarrange.verify_pos,
  createdjob.availability,
  jobworker.address
FROM 
  psnl.jobworker INNER JOIN cbo.jobarrange
  ON jobworker.uniqkey = jobarrange.empkey
  INNER JOIN cbo.projects
  ON jobarrange.project = projects.projid
  INNER JOIN psnl.offices
  ON jobarrange.offcloc = offices.offcid
  INNER JOIN psnl.createdjob
  ON jobarrange.psnl_ref = createdjob.psnl_no
WHERE 
  (jobarrange.psnl_ref = $P{ControlNo}) AND
  ((jobarrange.jobyear = $P{Anios}))
ORDER BY
  lastname, firstname, midname]]>
	</queryString>
