	</parameter>
	<queryString>
		<![CDATA[SELECT office, projtitle, COUNT(*), jobyear FROM (
   SELECT DISTINCT
      offices.office, 
      projects.projtitle, 
      jobarrange.empkey,
      jobarrange.jobyear
   FROM
      psnl.offices INNER JOIN cbo.jobarrange
      ON offices.offcid = jobarrange.offcloc
      INNER JOIN cbo.projects
      ON projects.projid = jobarrange.project
   WHERE
      ((jobarrange.frdate BETWEEN $P{DateFr} AND $P{DateTo}) OR
       (jobarrange.todate BETWEEN $P{DateFr} AND $P{DateTo}))
) GROUP BY
   office,
   projtitle,
   jobyear
ORDER BY
   office,
   projtitle]]>
	</queryString>
