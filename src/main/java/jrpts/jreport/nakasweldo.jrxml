	<queryString>
		<![CDATA[WITH payroll AS (
	SELECT worker, opesina, 'JOS' AS tyepo FROM pay.timebook WHERE (opesina <> '1021') AND ($X{IN, acctg_ref, ActgRefz})
	UNION ALL
	SELECT worker, opesina, 'COS' AS tyepo FROM pay.laborpaid WHERE (opesina <> '1021') AND ($X{IN, acctg_ref, ActgRefz})
)
SELECT 
	humane(jobworker.lastname, jobworker.firstname, jobworker.suffix, jobworker.midname) AS ngalan,
	jobworker.jobdesc,
	offices.office,
	payroll.tyepo
FROM
	psnl.jobworker INNER JOIN payroll
	ON jobworker.uniqkey = payroll.worker
	INNER JOIN psnl.offices
	ON offices.offcid = payroll.opesina
ORDER BY
	payroll.tyepo,
	offices.office,
	jobworker.lastname,
	jobworker.firstname,
	jobworker.midname]]>
	</queryString>
